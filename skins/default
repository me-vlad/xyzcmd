# Default XYZ skin

AUTHOR: "Max E. Kuznecov <mek@mek.uz.ua>"
VERSION: "0.1"
DESCRIPTION: "Default XYZ skin"

fs.type {
	file = LIGHT_GRAY
	dir = WHITE
	block = DARK_MAGENTA
	char = LIGHT_MAGENTA
	link = LIGHT_CYAN
	fifo = DARK_CYAN
	socket = DARK_RED
}

fs.regexp {
	```*.core$``` = DARK_RED
	```\.+``` = LIGHT_GREY
}
