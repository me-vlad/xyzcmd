# 
# Default keybinds
#

load :core:keycodes:*
load :core:pluginlist:show_list
load :sys:launcher:*
load :sys:logger:*

load :misc:hello:say_hello

bind :sys:logger:show_console to F2
bind :sys:launcher:shutdown to F10
bind :core:pluginlist:show_list to F1
bind :misc:hello:say_hello to META-;
bind :core:keycodes:learn_keys to CTRL-k

#set chain key META-Q
#set chain key META-L context PANEL
#set chain key TAB-Q context TAB
