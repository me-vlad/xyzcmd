#
# Main configuration file. Default settings. Do not edit.
#

# Enabled plugins

plugins_on(":sys:run",
           ":sys:cmd",
           ":sys:panel",
           ":sys:logger",

           ":core:keycodes",
           ":core:pluginlist",
           ":core:bindlist",
           ":core:shell",
           ":core:console",

           ":misc:hello",
           ":misc:about",
           ":misc:where",

           ":ui:testinput",
           ":ui:bookmarks",

           ":vfs:fileinfo",
           ":vfs:vfsutils",

           ":fsrules:magic",
           )

# Skin to use
let("skin", "seablue", sect="xyz")

# Terminal library
let("term_lib", "raw", sect="xyz")

# Colors to use. One of 1, 16, 88 256
let("term_colors", 16, sect="xyz")

# System shell
let("shell", env("SHELL", "/bin/sh"), sect="xyz")

# Local encoding
# One of the available in python
# (http://docs.python.org/lib/standard-encodings.html)
#let("local_encoding", "utf-8", sect="xyz")
