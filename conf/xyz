# Enabled plugins
# Syntax is:
#   {plugin_ns_path} > {ENABLE | DISABLE}

plugins {
	:misc:hello > ENABLE
	:core:keycodes > ENABLE
	:core:pluginlist > ENABLE
}

# Skin to use
skin = monochrome
#skin = seablue

# Log levels to track
log_level = ERROR,WARNING,INFO

# Maximum number of entries to keep
log_lines = 100

# Command line prompt
